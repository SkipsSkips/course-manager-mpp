{
  "name": "mppbackend",
  "version": "1.0.0",
  "description": "Backend for MPP application with PostgreSQL integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",
    "setup": "npm install && npm run migrate && npm run seed"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.35.0",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "multer": "^1.4.5-lts.1",
    "winston": "^3.11.0",
    "sqlite3": "^5.1.6",
    "@faker-js/faker": "^8.4.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.17.47",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.16",
    "concurrently": "^8.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "sequelize-cli": "^6.6.2"
  },
  "keywords": [
    "express",
    "postgresql",
    "sequelize",
    "rest-api",
    "backend"
  ],
  "author": "Your Name",
  "license": "MIT"
}
